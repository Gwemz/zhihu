page{
  font-family:"微软雅黑";
  background-color:#f5f5f5;
}
.swiper {
  height: 400rpx;
}
.list-item{
  box-sizing: border-box;
  margin:10rpx 0;
  padding:30rpx;
  height:200rpx;
  border-bottom:1px solid #ccc;
  border-radius:30rpx;
  background-color:#fff;
}
.content-title{
  width:450rpx;
  float:left;
  font-size:12pt;
}
.content-image{
  width:140rpx;
  height:140rpx;
  float:right;
}
page{
  box-sizing:border-box;
  position: absolute;
  top:0;
  bottom:0;
  left:0;
  right:0;
  margin:auto;
}

.content{
  box-sizing:border-box;
  position: absolute;
  top:0;
  bottom:0;
  left:0;
  right:0;
  margin:auto;
}

.title {
  position: absolute;
  width: 100%;
  box-sizing: border-box;
  padding: 0 30rpx;
  color: white;
  bottom: 50rpx;
  left: 0;
}
.list_left{
  position:fixed;
  top:0;
  left:0;
  bottom:0;
  width:65%;
  background-color:#232a30;
  transform:translateX(-100%);
  overflowX:hidden;
  overflowY:auto;
}
.list_left .block{
  padding:20rpx;
  text-align:center;
  height:80rpx;
  line-height:80rpx;
  font-size:25rpx;
  color:#fff;
  border-bottom:1px solid #1b2329;
}
.list_cir{
  position:fixed;
  right:30rpx;
  bottom:30rpx;
  border-radius:50%;
  width:100rpx;
  height:100rpx;
  background-color:blue;
  box-shadow:5px 5px 5px rgba(0,0,0,.5);
}